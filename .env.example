# Authentication
# Port to expose on the host
APP_PORT=8050
API_KEY=your_secure_password

# HuggingFace Settings
HUGGINGFACE=false
HUGGINGFACE_API_KEY=

# Vertex AI Credentials
# Directory for service account credential files
CREDENTIALS_DIR=/app/credentials
# JSON string for service account credentials (optional if using files in CREDENTIALS_DIR)
GOOGLE_CREDENTIALS_JSON=
# API Key for Vertex Express Mode (comma-separated for multiple keys)
VERTEX_EXPRESS_API_KEY=

# Streaming Settings
FAKE_STREAMING=false
FAKE_STREAMING_INTERVAL=1.0

# Model Configuration
MODELS_CONFIG_URL=https://raw.githubusercontent.com/gzzhongqi/vertex2openai/refs/heads/main/vertexModels.json

# Load Balancing
ROUNDROBIN=false

# Safety Settings
SAFETY_SCORE=false

# Proxy Settings
PROXY_URL=
SSL_CERT_FILE=